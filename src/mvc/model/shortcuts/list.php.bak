/** @var $model \bbn\mvc\model*/
$menu = new \bbn\appui\menus($model->inc->options, $model->inc->pref);
$shortcuts = $menu->shortcuts($model->inc->pref);
$prepath = $model->get_prepath();
if ( !empty($prepath) ){
  return array_map(function($a) use($prepath){
    if ( strpos($a['link'], $prepath) === 0 ){
      $a['link'] = substr($a['link'], strlen($prepath)+1);
    }
    return $a;
  }, $shortcuts);
}
return $shortcuts;
